{
  "name": "marketplace-webapp",
  "description": "Decentraland's LAND Marketplace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development node scripts/start.js",
    "pretest": "npm run lint",
    "lint": "./node_modules/eslint/bin/eslint.js src/",
    "lint:fix": "./node_modules/eslint/bin/eslint.js --fix src/",
    "test": "CI=true node scripts/test.js --env=jsdom",
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "NODE_ENV=production REACT_APP_VERSION=$(git rev-parse --short HEAD) node scripts/build.js",
    "postbuild": "export bundle_path=`ls ./build/static/js/*.js` && uglifyjs $bundle_path --compress --mangle reserved=['toParcelObject','normalizeParcel','connectParcel','getParcelPublications','buildCoordinate']  --output $bundle_path",
    "remotebuild": "REACT_APP_VERSION=$(git rev-parse --short HEAD) NODE_PATH=/tmp/auction/webapp/node_modules node scripts/build.js"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "axios": "^0.17.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "date-fns": "^1.29.0",
    "decentraland-commons": "^4.0.1",
    "decentraland-dapps": "^3.4.2",
    "decentraland-eth": "^7.1.0",
    "decentraland-ui": "^1.10.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "7.4.0",
    "ethereum-blockies": "^0.1.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "impetus": "^0.8.7",
    "jest": "20.0.4",
    "lodash.debounce": "^4.0.8",
    "mouse-wheel": "^1.2.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "1.12.1",
    "promise": "8.0.1",
    "prop-types": "^15.5.10",
    "query-string": "^5.1.0",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-circular-progressbar": "^1.0.0",
    "react-dev-utils": "^5.0.1",
    "react-dom": "16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-virtualized": "^9.18.5",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-storage": "^4.1.2",
    "redux-storage-decorator-filter": "^1.1.8",
    "redux-storage-engine-localstorage": "^1.1.4",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "resolve": "1.6.0",
    "semantic-ui-css": "^2.2.14",
    "semantic-ui-react": "^0.78.2",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "touch-pinch": "^1.0.1",
    "touch-position": "^2.0.0",
    "uglify-es": "^3.3.9",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "source-map-explorer": "^1.5.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
