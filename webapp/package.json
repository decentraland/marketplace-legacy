{
  "name": "Marketplace-webapp",
  "description": "Decentraland's LAND Marketplace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "pretest": "npm run lint",
    "lint": "./node_modules/eslint/bin/eslint.js src/",
    "lint:fix": "./node_modules/eslint/bin/eslint.js --fix src/",
    "test": "CI=true react-scripts test --env=jsdom",
    "compile-deps":
      "npx babel-cli ./node_modules/decentraland-eth --out-dir ./node_modules/decentraland-eth && npx babel-cli ./node_modules/web3-provider-engine --out-dir ./node_modules/web3-provider-engine",
    "prebuild": "npm run test && npm run compile-deps",
    "analyze": "source-map-explorer build/static/js/main.*",
    "build":
      "REACT_APP_VERSION=$(git rev-parse --short HEAD) react-scripts build",
    "remotebuild":
      "REACT_APP_VERSION=$(git rev-parse --short HEAD) NODE_PATH=/tmp/auction/webapp/node_modules react-scripts build",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "babel-eslint": "^8.2.1",
    "babel-polyfill": "^6.26.0",
    "date-fns": "^1.29.0",
    "decentraland-commons": "^4.0.1",
    "decentraland-eth": "^2.1.0",
    "eslint": "4.10.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "ethereum-blockies": "^0.1.1",
    "history": "^4.7.2",
    "impetus": "^0.8.7",
    "lodash.debounce": "^4.0.8",
    "mouse-wheel": "^1.2.0",
    "prettier": "^1.10.2",
    "prop-types": "^15.5.10",
    "query-string": "^5.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-scripts": "^1.1.4",
    "react-virtualized": "^9.18.5",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-storage": "^4.1.2",
    "redux-storage-decorator-filter": "^1.1.8",
    "redux-storage-engine-localstorage": "^1.1.4",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "semantic-ui-css": "^2.2.14",
    "semantic-ui-react": "^0.78.2",
    "touch-pinch": "^1.0.1",
    "touch-position": "^2.0.0",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "source-map-explorer": "^1.5.0"
  }
}
